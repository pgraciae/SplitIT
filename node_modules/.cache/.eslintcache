 'Home'){\n      const videoConstraints = {\n        width: 1280,\n        height: 720,\n        facingMode: \"user\"\n      };\n    return(\n    <div className=\"App\">\n        <header className=\"App-header\">\n      <NavBarCustom view={this.go}></NavBarCustom>\n        <Stack direction=\"row\" spacing={2}>\n      <input\n        accept=\"image/*\"\n        style={{ display: 'none' }}\n        id=\"raised-button-file\"\n        multiple\n        type=\"file\"\n        onChange={(event) => this.loadImage(event)}\n      />\n    <label htmlFor=\"raised-button-file\">\n      <Button variant=\"contained\" component=\"span\" style = {{position: \"absolute\", top: '10vw', left: '35vw', width: '10vw', height: '5vw'}} >\n        Upload\n      </Button>\n    </label>\n\n      <Button variant=\"contained\" onClick ={this.button_test} style = {{position: \"absolute\", top: '10vw', left: '45vw', width: '10vw', height: '5vw'}}>\n        NO SABIA SI PODIA TREURE\n      </Button>\n      {this.state.webcamEnabled &&\n      <Webcam\n          audio={false}\n          height={200}\n          ref={this.setRef}\n          screenshotFormat=\"image/jpeg\"\n          width={350}\n          videoConstraints={videoConstraints}\n        />}\n      <Button variant =\"contained\" onClick={this.enableWebcam} style = {{position: \"absolute\", top: '10vw', left: '55vw', width: '10vw', height: '5vw'}}>\n        Camera\n      </Button>\n    </Stack>\n    <div style = {{position: 'absolute', width: '70vw', left: '22vw', top:'60vw'}}>\n      <TableX></TableX>\n      </div>\n      </header>\n\n    </div>\n    )\n  }\n  else if (this.state.View === 'Profile'){\n    return(\n      <div className=\"App\">\n      <header className=\"App-header\">\n      <div>\n        <NavBarCustom view={this.go}></NavBarCustom>\n      </div>\n      <ProfileView></ProfileView>\n      </header>\n\n      </div>\n\n    )\n  }\n  else if (this.state.View === 'Tickets'){\n    return(\n      <div className=\"App\">\n      <header className=\"App-header\">\n      <div>\n        <NavBarCustom view={this.go}></NavBarCustom>\n      </div>\n      </header>\n      </div>\n    )\n  }\n}\n\n}\n\n\n\nexport default App;","/home/pgraciae/enginyeriadades/TFG/SplitIT/src/Components/Table.js",["42"],"import React from \"react\";\nimport ReactDOM from \"react-dom\";\nimport { makeStyles } from \"@material-ui/core/styles\";\nimport Table from \"@material-ui/core/Table\";\nimport TableBody from \"@material-ui/core/TableBody\";\nimport TableCell from \"@material-ui/core/TableCell\";\nimport TableHead from \"@material-ui/core/TableHead\";\nimport TableRow from \"@material-ui/core/TableRow\";\nimport Input from \"@material-ui/core/Input\";\nimport Paper from \"@material-ui/core/Paper\";\nimport IconButton from \"@material-ui/core/IconButton\";\n// Icons\nimport EditIcon from \"@material-ui/icons/EditOutlined\";\nimport DoneIcon from \"@material-ui/icons/DoneAllTwoTone\";\nimport RevertIcon from \"@material-ui/icons/NotInterestedOutlined\";\n\nconst useStyles = makeStyles((theme) => ({\n  root: {\n    width: \"100%\",\n    marginTop: theme.spacing(3),\n    overflowX: \"auto\"\n  },\n  table: {\n    minWidth: 650\n  },\n  selectTableCell: {\n    width: 60\n  },\n  tableCell: {\n    width: 130,\n    height: 40\n  },\n  input: {\n    width: 130,\n    height: 40\n  }\n}));\n\nconst createData = (Quantity, ProductName, Price) => ({\n  id: ProductName.replace(\" \", \"_\"),\n  Quantity, ProductName, Price,isEditMode: false\n});\n\nconst CustomTableCell = ({ row, name, onChange }) => {\n  const classes = useStyles();\n  const { isEditMode } = row;\n  return (\n    <TableCell align=\"left\" className={classes.tableCell}>\n      {isEditMode ? (\n        <Input\n          value={row[name]}\n          name={name}\n          onChange={(e) => onChange(e, row)}\n          className={classes.input}\n        />\n      ) : (\n        row[name]\n      )}\n    </TableCell>\n  );\n};\n\n function TableX() {\n  const [rows, setRows] = React.useState([\n    createData(159 , \"Frozen yoghurt\", 24,),\n    createData(237, \"Ice cream sandwich\",34),\n    createData(262 , \"Eclair\", 12 )\n  ]);\n  const [previous, setPrevious] = React.useState({});\n  const classes = useStyles();\n\n  const onToggleEditMode = (id) => {\n    setRows((state) => {\n      return rows.map((row) => {\n        if (row.id === id) {\n          return { ...row, isEditMode: !row.isEditMode };\n        }\n        return row;\n      });\n    });\n  };\n\n  const onChange = (e, row) => {\n    if (!previous[row.id]) {\n      setPrevious((state) => ({ ...state, [row.id]: row }));\n    }\n    const value = e.target.value;\n    const name = e.target.name;\n    const { id } = row;\n    const newRows = rows.map((row) => {\n      if (row.id === id) {\n        return { ...row, [name]: value };\n      }\n      return row;\n    });\n    setRows(newRows);\n  };\n\n  const onRevert = (id) => {\n    const newRows = rows.map((row) => {\n      if (row.id === id) {\n        return previous[id] ? previous[id] : row;\n      }\n      return row;\n    });\n    setRows(newRows);\n    setPrevious((state) => {\n      delete state[id];\n      return state;\n    });\n    onToggleEditMode(id);\n  };\n\n  return (\n    <Paper className={classes.root}>\n      <Table className={classes.table} aria-label=\"caption table\">\n        <TableHead>\n          <TableRow>\n            <TableCell align=\"left\" />\n            <TableCell align=\"left\">Quantity</TableCell>\n            <TableCell align=\"left\">Product Name</TableCell>\n            <TableCell align=\"left\">Price</TableCell>\n            \n          </TableRow>\n        </TableHead>\n        <TableBody>\n          {rows.map((row) => (\n            <TableRow key={row.id}>\n              <TableCell className={classes.selectTableCell}>\n                {row.isEditMode ? (\n                  <>\n                    <IconButton\n                      aria-label=\"done\"\n                      onClick={() => onToggleEditMode(row.id)}\n                    >\n                      <DoneIcon />\n                    </IconButton>\n                    <IconButton\n                      aria-label=\"revert\"\n                      onClick={() => onRevert(row.id)}\n                    >\n                      <RevertIcon />\n                    </IconButton>\n                  </>\n                ) : (\n                  <IconButton\n                    aria-label=\"delete\"\n                    onClick={() => onToggleEditMode(row.id)}\n                  >\n                    <EditIcon />\n                  </IconButton>\n                )}\n              </TableCell>\n              <CustomTableCell {...{ row, name: \"Quantity\", onChange }} />\n              <CustomTableCell {...{ row, name: \"ProductName\", onChange }} />\n              <CustomTableCell {...{ row, name: \"Price\", onChange }} />\n            </TableRow>\n          ))}\n        </TableBody>\n      </Table>\n    </Paper>\n  );\n}\n\n\nexport default TableX;","/home/pgraciae/enginyeriadades/TFG/SplitIT/src/Components/uploadFile.js",["43","44"],"\nimport React,{Component} from 'react';\nimport Button from '@mui/material/Button';\n//import input from '@mui/material/input';\n\n\nfunction inputFile(props){\n\n    return (\n    <input onChange ={(event)=>props.img(event)} type=\"file\"> Gallery </input>\n        )\n}\n\nexport default inputFile;","/home/pgraciae/enginyeriadades/TFG/SplitIT/src/Components/NavBar.js",["45","46","47"],"\nimport Container from 'react-bootstrap/Container'\nimport { Navbar, Nav, NavItem, NavDropdown, MenuItem } from 'react-bootstrap';\nimport 'bootstrap/dist/css/bootstrap.min.css' ;\n\nfunction NavBarCustom(props) {\n\n    return (\n        <div style = {{position: 'absolute', width: '100%', left: '0vw', top:'0vw'}}>\n  <Navbar bg=\"dark\" variant=\"dark\" >\n    <Container style = {{height:'3vw'}}>\n    <Nav className=\"me-auto\" >\n      <Nav.Link href=\"#home\" style = {{